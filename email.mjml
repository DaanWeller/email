        <mj-column>
          <mj-divider border-width="1px" border-style="solid" border-color="lightgrey" />
        </mj-column>
      </mj-section>

      <!-- Columns -->

      <mj-include path="./Elements/EventBlock.mjml" /> <!-- or 'header.mjml' -->



      <!-- Columns 2 -->

      <mj-include path="./Elements/EventBlock.mjml" /> <!-- or 'header.mjml' -->



      <!-- Sponsoren -->
      <mj-section background-color="#fafafa">

